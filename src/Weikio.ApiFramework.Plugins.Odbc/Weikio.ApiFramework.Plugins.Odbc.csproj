    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Authors>Weik.io</Authors>
    <Company>Weik.io</Company>
    <Description>ODBC plugin for Api Framework.</Description>
    <PackageDescription>ODBC plugin for Api Framework.</PackageDescription>
    <PackageProjectUrl>https://www.weikio.com</PackageProjectUrl>
    <Copyright>Â©2021 Weik.io</Copyright>
    <PackageId>Weikio.ApiFramework.Plugins.Odbc</PackageId>
    <Product>Weikio.ApiFramework.Plugins.Odbc</Product>
    <AssemblyName>Weikio.ApiFramework.Plugins.Odbc</AssemblyName>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <RepositoryUrl>https://github.com/weikio/ApiFramework.Plugins.Odbc</RepositoryUrl>
    <PackageTags>apiframework;plugin;odbc</PackageTags>
    <PackageIcon>logo_256.png</PackageIcon>
    <Title>ODBC plugin for Api Framework.</Title>
    <MinVerMinimumMajorMinor>1.0</MinVerMinimumMajorMinor>    
